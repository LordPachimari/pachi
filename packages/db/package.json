{
  "name": "@pachi/db",
  "version": "0.1.0",
  "private": true,
  "main": "./index.ts",
  "types": "./index.ts",
  "license": "MIT",
  "scripts": {
    "beautify": "prettier \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --write --log-level error",
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "generate": "drizzle-kit generate:pg",
    "push": "drizzle-kit push:pg",
    "studio": "drizzle-kit studio --port 3001 --verbose",
    "dev": "drizzle-kit studio",
    "migrate": "wrangler d1 migrations apply local --local",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.6.0",
    "drizzle-orm": "^0.30.4",
    "drizzle-valibot": "^0.1.1",
    "drizzle-zod": "^0.5.1",
    "pg": "^8.11.3",
    "replicache": "14.1.0",
    "valibot": "^0.17.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@pachi/eslint-config": "workspace:^0.1.0",
    "@pachi/prettier-config": "workspace:^0.1.0",
    "@pachi/tsconfig": "workspace:^0.1.0",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.20.6",
    "eslint": "^8.53.0",
    "prettier": "^3.0.3",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@pachi/eslint-config/base"
    ]
  },
  "prettier": "@pachi/prettier-config"
}
