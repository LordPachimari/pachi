{
  "extends": "@pachi/tsconfig/base.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "paths": {
      "~/*": ["./src/*"]
    },
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "lib": ["esnext"],
    "jsx": "react-jsx"
  },
  "include": ["."],
  "exclude": ["node_modules"]
}
