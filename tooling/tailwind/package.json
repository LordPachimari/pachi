{
  "name": "@pachi/tailwind-config",
  "version": "0.1.0",
  "private": true,
  "main": "index.ts",
  "license": "MIT",
  "files": [
    "index.ts",
    "postcss.js"
  ],
  "scripts": {
    "beautify": "prettier \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --write --log-level error",
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "8.4.30",
    "tailwindcss": "3.4.1"
  },
  "devDependencies": {
    "@medusajs/ui-preset": "^1.0.1",
    "@pachi/eslint-config": "workspace:^0.1.0",
    "@pachi/prettier-config": "^0.1.0",
    "@pachi/tsconfig": "^0.1.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "eslint": "^8.53.0",
    "prettier": "^3.0.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@pachi/eslint-config/base"
    ]
  },
  "prettier": "@pachi/prettier-config"
}
