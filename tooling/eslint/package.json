{
  "name": "@pachi/eslint-config",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "files": [
    "./base.js",
    "./nextjs.js",
    "./react.js"
  ],
  "scripts": {
    "beautify": "prettier \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --write --log-level error",
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^14.0.1",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-turbo": "^1.10.16",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "devDependencies": {
    "@pachi/prettier-config": "workspace:^0.1.0",
    "@pachi/tsconfig": "workspace:^0.1.0",
    "@stylistic/eslint-plugin": "^1.6.3",
    "@stylistic/eslint-plugin-migrate": "^1.6.3",
    "@types/eslint": "^8.44.7",
    "eslint": "^8.53.0",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "./base.js"
    ]
  },
  "prettier": "@pachi/prettier-config"
}
