    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6"
  },
  "devDependencies": {
    "@pachi/tsconfig": "workspace:^0.1.0",
    "typescript": "^5.3.3"
  },
  "prettier": "@pachi/prettier-config"
}
